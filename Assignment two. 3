public class transpose
{
    public static void main(String args[])
    {
        int a[][]={{4, 2,6}, {6, 8,9}, {10, 12,13}};
        int t[][]=new int[3][3];
        for(int i=0;i<3;i++)
        {
            for(int j=0;j<3;j++)
            {
                t[i][j]=a[j][i];
            }
        }
        System.out.println("Before transpose");
        for(int i=0;i<3;i++)
        {
            for(int j=0;j<3;j++)
        {
            System. out. println(a[i] [j]+" ");
        }
        System. out. println();
        }
        System. out. println("After transpose");
        for(int i=0;i<3;i++)
        {
            for(int j=0;j<3;j++)
            {
                System. out. println(t[i] [j]+" ");
            }
            System. out. println();
        }
            
        }
    }
